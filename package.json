{
  "name": "prompt-lang",
  "displayName": "prompt-lang",
  "description": "syntax highlighting for .prompt files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "prompt",
        "aliases": [
          "prompt",
          "prompt"
        ],
        "extensions": [
          ".prompt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "prompt",
        "scopeName": "source.prompt",
        "path": "./syntaxes/prompt.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/max-hans/prompt-lang-extension.git"
  },
  "bugs": {
    "url": "https://github.com/max-hans/prompt-lang-extension/issues"
  }
}
